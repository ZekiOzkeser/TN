
@{
    Layout = null;
}
<!DOCTYPE html>
<html dir="ltr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/img/logo.png">
    <title>Hata!!!</title>
    <link href="~/Content/Error/css/style.min.css" rel="stylesheet" />


</head>
<body>
    <div class="main-wrapper">
        <div class="preloader">
            <div class="lds-ripple">
                <div class="lds-pos"></div>
                <div class="lds-pos"></div>
            </div>
        </div>
        <div class="error-box">
            <div class="error-body text-center">
                <h1 class="error-title text-danger">HATA !!</h1>
                <h3 class="text-uppercase error-subtitle">Sayfa bulunamadı !</h3>
                <h6 class="text-uppercase error-subtitle">Kullanıcı girişi yapınız..</h6>
                <p class="text-muted m-t-30 m-b-30">@ViewBag.hata</p>
                <a href="@Url.Action("Index","Home")" class="btn btn-danger btn-rounded waves-effect waves-light m-b-40">Geri dön</a>
            </div>
        </div>
    </div>
    <script src="~/Content/Error/jquery/dist/jquery.min.js"></script>
    
    <script src="~/Content/Error/popper.min.js"></script>

    <script src="~/Content/Error/bootstrap.min.js"></script>
    <script>
        $('[data-toggle="tooltip"]').tooltip();
        $(".preloader").fadeOut();
    </script>
</body>
</html>